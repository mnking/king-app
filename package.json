{
  "name": "react-microservices-frontend-framework",
  "private": false,
  "version": "1.1.12",
  "type": "module",
  "description": "Production-ready React TypeScript frontend framework for TOS",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "frontend-framework",
    "msw",
    "rest-api"
  ],
  "author": "Frontend Framework Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run",
    "test:unit": "vitest run",
    "test:all": "RUN_INTEGRATION_TESTS=true vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "clean": "rm -rf dist node_modules",
    "cleanup-imports": "node scripts/cleanup-unused-imports.js",
    "framework:setup": "cp .env.template .env && echo 'Framework setup complete! Edit .env for your project.'",
    "backup": "backup-scripts/backup-local-files.sh",
    "restore": "backup-scripts/restore-local-files.sh",
    "restore:list": "backup-scripts/restore-local-files.sh --list"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-table": "^8.21.3",
    "class-names": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.49.3",
    "react-hot-toast": "^2.6.0",
    "react-is": "^18.3.1",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^6.21.3",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.4.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.1",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.1.0",
    "@vitest/ui": "^3.2.4",
    "agentation": "^1.1.0",
    "autoprefixer": "^10.4.18",
    "baseline-browser-mapping": "^2.9.14",
    "dotenv": "^16.4.7",
    "eslint": "^9.9.1",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-testing-library": "^7.11.0",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.2",
    "postcss": "^8.4.35",
    "shadcn": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
